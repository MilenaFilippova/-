#include <locale.h>
#include <iostream>
using namespace std;
int main(void) 
{
	const int l=10,m=10;
	int s=0;
	int Q[l][m];
	int min=0,max=10;
	for (int i=0; i<l; i++)
	{
		for (int j=0;j<m; j++)
		{
			Q[i][j]= min+rand()%(max-min+1);
			s=s+Q[i][j];
			s=s/m;
		}
		for (int j=0;j<m; j++)
		{
		    cout <<Q[i][j]-s <<'\t';
		}
		
        cout << endl;
	}

	return 0;
}

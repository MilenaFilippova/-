#include <locale.h>
#include <iostream>
using namespace std;
int main(void) 
{
	const int N=10;
	int A[N];
	int min=-10,max=10,sum;
	int countN=0, sum=0;
	for (int i=0; i<N; i++) 
	{
		A[i]= min+rand()%(max-min+1);
	}
	for (int i=0; i<N;i++)
	{
		if(A [1]<0)
		countN++;
		else
		sum+=A[i];
	}
	
			cout << countN << endl<<sum ;
	return 0;
}
